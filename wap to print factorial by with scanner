import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
    Scanner scan = new Scanner (System.in);
    System.out.print("enter the number : ");
    int num =scan.nextInt();
    

	// decresing order 
	int fact = 1;
	for (int i =num;i>=1;i--)
	{
	fact *=i;
	}
	
	System.out.println(num+"! ="+fact);
	}
}
